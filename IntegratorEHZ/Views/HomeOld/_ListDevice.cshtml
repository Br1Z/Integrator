@model IEnumerable<IntegratorEHZ.Models.Device>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
@if (Model.Count() > 0)
{
    @foreach (var item in Model)
    {
        <button type="button" data-url="@Url.Action("TabDevice","Home",new { id= item.Id })" class="list-group-item list-group-item-action deviceButton">@item.IMEI</button>
    }
}
else
{
    <p>Не одно устройство ещё не создано</p>
}

<script>
    $(".deviceButton").click(function () {
        var url = $(this).data("url");
        $('#ValuesList').load(url);
    });
</script>
